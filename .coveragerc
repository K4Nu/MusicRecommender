[run]
source = users,utils
omit =
    */migrations/*
    */tests/*
    */tasks/*
    */admin.py
    */__init__.py
    */apps.py
    manage.py
    */settings/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod